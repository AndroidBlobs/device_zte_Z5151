on post-fs-data
	# add for high cpu freq in boot
	write	/sys/devices/system/cpu/cpu0/core_ctl/max_cpus 4
	write	/sys/devices/system/cpu/cpu0/core_ctl/min_cpus 4
	write	/sys/devices/system/cpu/cpu4/core_ctl/max_cpus 4
	write	/sys/devices/system/cpu/cpu4/core_ctl/min_cpus 4
	write   /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1401000
	write   /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 1401000
	write   /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 1401000
	write   /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 1401000
	write   /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 1401000
	write   /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq 1401000
	write   /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq 1401000
	write   /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq 1401000
	write   /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 1094400
	write   /sys/devices/system/cpu/cpu5/cpufreq/scaling_max_freq 1094400
	write   /sys/devices/system/cpu/cpu6/cpufreq/scaling_max_freq 1094400
	write   /sys/devices/system/cpu/cpu7/cpufreq/scaling_max_freq 1094400
	write   /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 1094400
	write   /sys/devices/system/cpu/cpu5/cpufreq/scaling_min_freq 1094400
	write   /sys/devices/system/cpu/cpu6/cpufreq/scaling_min_freq 1094400
	write   /sys/devices/system/cpu/cpu7/cpufreq/scaling_min_freq 1094400
